generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RagSource {
  source_id  String  @id
  filename   String?
  title      String?
  author     String?
  year       Int?
  edition    String?
  publisher  String?
  source_url String?
}
